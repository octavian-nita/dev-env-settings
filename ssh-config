ProxyCommand /bin/connect.exe -H <proxy>:<port> %h %p

# Also execute the following:
# export HTTP_PROXY_USER=proxy-username
# export HTTP_PROXY_PASSWORD=proxy-password

Host github.com
User git
Port 22
Hostname github.com
IdentityFile "C:\Users\nitanoc\.ssh\id_rsa"
TCPKeepAlive yes
IdentitiesOnly yes

Host ssh.github.com
User git
Port 443
Hostname ssh.github.com
IdentityFile "C:\Users\nitanoc\.ssh\id_rsa"
TCPKeepAlive yes
IdentitiesOnly yes
